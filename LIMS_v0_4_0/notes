# set up virtual env
download python
pip install virtualenv
pip install virtualenvwrapper-win

# virtual env

## make virtual env
mkvirtualenv LIMS_dev_env

## download all requirements to virtual env
pip install -r requirements.txt

## To launch
workon LIMS_dev_env

## to deactivate
deactivate

#Versions and dependicies
python 3.5.2
django 2.1.5
crispy-forms
django-bootstrap4 ("bootstrap4")
django-tables2
## excel dependicies
xlwt - for working with excel files
django-excel
pyexcel-xls
django-filters
